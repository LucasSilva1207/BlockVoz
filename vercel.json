{
    "version": 2,
    "builds": [
      {
        "src": "Back-end/server.js", 
        "use": "@vercel/node"
      },
      {
        "src": "App-Quiz-Javascript-master/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/routes/(.*)",
        "dest": "/Back-end/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/App-Quiz-Javascript-master/$1"
      },
      {
        "src": "/",
        "dest": "/App-Quiz-Javascript-master/login.html"
      }
    ]
  }
  